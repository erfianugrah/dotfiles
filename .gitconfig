[http]
	postBuffer = 524288000
	version = HTTP/2
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = main
[credential "https://dev.azure.com"]
	useHttpPath = true
[credential]
	credentialStore = cache
[user]
	signingkey = BC4056524A00EAB4
[commit]
	gpgsign = true
